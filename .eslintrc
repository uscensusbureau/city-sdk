{
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "CitySDK": true
  },
  "rules": {
    "camelcase": [0],
    "comma-spacing": [0],
    "consistent-return": [0],
    "curly": [0],
    "eol-last": [0],
    "max-len": [1, 360],
    "key-spacing": [0],
    "new-cap": [0],
    "no-mixed-requires": [0],
    "no-redeclare": [2],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0],
    "no-unused-expressions": [0],
    "no-extra-semi": [0],
    "semi": [0],
    "space-infix-ops": [0],
    "quotes": [0],
    "strict": [0],
    "eqeqeq": [0],
    "valid-typeof": 2
  }
}
